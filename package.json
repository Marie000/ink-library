{
  "name": "ink-library",
  "version": "0.1.0",
  "description": "The ink library frontend",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "dev": "SECRETORKEY=kick-opossum-snowiness ISSUER=auth.reader-api.test API_SERVER=http://localhost:8080/ sapper dev",
    "build": "sapper build --legacy && snowpack --dest static/web_modules/ --optimize",
    "export": "sapper export --legacy",
    "start": "node __sapper__/build",
    "snowpack": "snowpack --dest static/web_modules/ --babel",
    "lint": "healthier && prettier --write --plugin-search-dir=. '**/*.{js,json,css,yml,svelte}'",
    "pretest": "npm run lint",
    "prepublishOnly": "npm run lint",
    "snapshots": "TAP_SNAPSHOT=1 npm run test && prettier --write --plugin-search-dir=. '**/*.{js,json,css,yml,svelte}'",
    "test": "jest src",
    "test:watch": "npm run test -- --watch"
  },
  "snowpack": {
    "webDependencies": [
      "intersection-observer",
      "inert-polyfill"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {},
      "postcss-easy-import": {},
      "postcss-calc": {},
      "cssnano": {}
    }
  },
  "jest": {
    "transform": {
      "^.+\\.js$": "babel-jest",
      "^.+\\.svelte$": "svelte-jester"
    },
    "moduleFileExtensions": [
      "js",
      "svelte"
    ]
  },
  "engines": {
    "node": "10"
  },
  "healthier": {
    "ignore": [
      "__sapper__",
      "rollup.config.js"
    ]
  },
  "dependencies": {
    "@google-cloud/storage": "^4.3.0",
    "autoprefixer": "^9.7.3",
    "connect-redis": "^4.0.3",
    "cssnano": "^4.1.10",
    "csurf": "^1.10.0",
    "date-fns": "^2.9.0",
    "debug": "^4.1.1",
    "document-register-element": "^1.14.3",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "got": "^10.0.1",
    "inert-polyfill": "^0.2.5",
    "jsonwebtoken": "^8.5.1",
    "mime": "^2.4.4",
    "passport": "^0.4.1",
    "passport-auth0": "^1.3.1",
    "passport-http": "^0.3.0",
    "postcss": "^7.0.24",
    "postcss-calc": "^7.0.1",
    "postcss-custom-properties": "^9.0.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-reporter": "^6.0.1",
    "redis": "^2.8.0",
    "short-uuid": "^3.1.1",
    "universal-base64url": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/runtime": "^7.7.6",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/svelte": "^1.11.0",
    "babel-jest": "^24.9.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "faker": "^4.1.0",
    "friendly-words": "^1.1.10",
    "healthier": "3.2.0",
    "intersection-observer": "^0.7.0",
    "jest": "^24.9.0",
    "postcss-load-config": "^2.1.0",
    "prettier": "1.19.1",
    "prettier-plugin-svelte": "^0.7.0",
    "rollup": "^1.27.12",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-svelte": "^5.1.1",
    "rollup-plugin-terser": "^5.1.3",
    "sapper": "^0.27.9",
    "sirv": "^0.4.2",
    "snowpack": "^1.0.4",
    "svelte": "^3.17.1",
    "svelte-jester": "^1.0.3",
    "svelte-preprocess": "^3.3.0",
    "tap": "14.9.2",
    "tiny-lr": "^1.1.1",
    "typescript": "^3.7.2"
  }
}
